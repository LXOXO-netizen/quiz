{
  "name": "nodejs-quiz-app",
  "version": "1.0.0",
  "description": "A goal of this project is to provide openSUSE booth staff with a local instance of a quiz",
  "scripts": {
    "start": "nodemon src/server.js",
    "production": "node src/server.js",
    "build:css": "lessc public/less/styles.less public/styles.css",
    "watch:css": "lessc -w public/less/styles.less public/styles.css",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "lint": "eslint . --ext .js",
    "lint:check": "eslint . --ext .js"
  },
  "dependencies": {
    "@netlify/functions": "^4.0.0",
    "@types/express": "^5.0.2",
    "ejs": "^3.1.10",
    "express": "^4.20.0",
    "gettext-parser": "^8.0.0",
    "less": "^4.3.0",
    "node-gettext": "^3.0.0",
    "serverless-http": "^3.2.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "nodemon": "3.1.10",
    "prettier": "^3.5.3"
  },
  "keywords": []
}
